  <Target Name="BeforeBuild">
    <!-- 
      The target "Precompiled" defined in Orchard.proj at root level is depended on "Build" target 
      so it is safe to alter item group in "BeforeBuild" target.
    -->
    <ItemGroup>
      <!-- We define V8NativeFiles items and remove None items because we want to control how to copy V8 native. -->
      <V8NativeFiles Include="@(None)" Condition="$([System.String]::new('%(FullPath)').Contains('JavaScriptEngineSwitcher.V8.Native'))" />
      <None Remove="@(None)" Condition="$([System.String]::new('%(FullPath)').Contains('JavaScriptEngineSwitcher.V8.Native'))" />
    </ItemGroup>
  </Target>
  <Target Name="CopyV8Native" AfterTargets="Build" Condition="'$(NotCopyV8Native)' == ''">
    <!-- We don't supply PreCompiledDir property when we build on local machine with Visual Studio -->
    <PropertyGroup Condition="'$(PreCompiledDir)' == ''">
      <V8NativeTargetDir>$([System.IO.Path]::Combine($(MSBuildThisFileDirectory), '..\..\..\Orchard.Web\bin\x86\'))</V8NativeTargetDir>
    </PropertyGroup>
    <PropertyGroup Condition="'$(PreCompiledDir)' != ''">
      <V8NativeTargetDir>$([System.IO.Path]::Combine($(PreCompiledDir), 'bin\x86\'))</V8NativeTargetDir>
    </PropertyGroup>
    <Message Importance="High" 
        Text="Copying V8 native @(V8NativeFiles->Count()) files: @(V8NativeFiles->'%(FullPath)') to '$([System.IO.Path]::GetFullPath($(V8NativeTargetDir)))'" />
    <Copy SourceFiles="@(V8NativeFiles)" DestinationFolder="$(V8NativeTargetDir)" Condition="!Exists('$(V8NativeTargetDir)%(Filename)%(Extension)')" />
  </Target>
